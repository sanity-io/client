{
  "$schema": "https://typedoc.org/schema.json",
  "name": "@sanity/client",
  "entryPoints": [
    "./src/index.ts",
    "./src/csm/index.ts",
    "./src/stega/index.ts",
    "./src/media-library.ts"
  ],
  "entryPointStrategy": "expand",
  "json": "./docs/client-typedoc-output.json",
  "pretty": true,
  "tsconfig": "./tsconfig.json",
  "excludePrivate": true,
  "excludeInternal": true,
  "excludeProtected": true,
  "excludeNotDocumented": false,
  "excludeReferences": true,
  "includeVersion": true,
  "readme": "./README.md",
  "plugin": ["typedoc-plugin-missing-exports"],
  "validation": {
    "notDocumented": false,
    "invalidLink": true,
    "notExported": true
  },
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Client",
    "Configuration",
    "Queries",
    "Mutations",
    "Transactions",
    "Assets",
    "Releases",
    "Types",
    "Utilities",
    "*"
  ],
  "sort": ["source-order"],
  "kindSortOrder": [
    "Document",
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "EnumMember",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Variable",
    "Function",
    "Accessor",
    "Method",
    "Parameter",
    "TypeParameter",
    "TypeLiteral",
    "CallSignature",
    "ConstructorSignature",
    "IndexSignature",
    "GetSignature",
    "SetSignature"
  ],
  "treatWarningsAsErrors": false,
  "treatValidationWarningsAsErrors": false,
  "logLevel": "Warn",
  "skipErrorChecking": true,
  "searchInComments": true,
  "searchInDocuments": true
}